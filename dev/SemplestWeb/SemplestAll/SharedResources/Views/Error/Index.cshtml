@model List<Semplest.SharedResources.Controllers.ErrorModel>

@{
    Layout = null;
    ViewBag.Title = "Index";
}

   <table border =1><tr><th>User</th><th>Error Message</th><th>Time Stamp</th></tr>
  @foreach (Semplest.SharedResources.Controllers.ErrorModel item in Model)
  {
  <tr>
  <td>
    <div class="display-field">
        @Html.DisplayFor(model => item.User)
    </div>
    </td>
<td>
    <div class="display-field">
        @Html.DisplayFor(model => item.ErrorMessage)
    </div>
    </td>
    <td>
    <div class="display-field">
        @Html.DisplayFor(model => item.TimeStamp)
    </div>
    </td>
    </tr>
  }
  </table>   
