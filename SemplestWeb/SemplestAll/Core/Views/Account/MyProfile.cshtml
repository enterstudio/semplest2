<script>
    $(document).ready(function () {
        var dataSource = new kendo.data.DataSource({
            transport: {
                read: {
                    Url: ''
                }
            },
            schema: {
                model: {

                    fields: {

                    }
                }
            },
            batch: true,
            pageSize: 10
        });
        $("#CredcardinfoGrid").kendoGrid({
            dataSource: dataSource,
            pageable: true,
            height: 150,
            scrollable: true,
            columns: [
            {
                title: "NickName",
                field: ""
            },
            {
                title: "Account#",
                field: ""
            },
            {
                title: "Action",
                Command: ["edit", "destroy"]
            }
            ]
        });
    });
</script>
