<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
        <link href="@Url.Content("~/Content/example.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/kendo.common.min.css")" rel="Stylesheet" />
        <link href="@Url.Content("~/Content/kendo.metro.min.css")"  rel="Stylesheet" />
        <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/jquery-ui.min.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/kendo.web.min.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/jquery.UiBlock.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"> </script>
    </head>
    <body class="centerWrap">
        <div id="header">
            <div class="centerWrap floatWrap">
                <img src="@Url.Content("~/Content/Metro/semplest_logo.png")"/>
                <ul class="kendo-nav">
                    <li style="margin-left: 0;">@Html.ActionLink("Create User", "Index", "Home", new {area = ""}, null)</li>
                    <li>
                        @Html.ActionLink("Campaign", "CampaignSetup", "Campaign")</li>
                    <li>@Html.ActionLink("Search Keywords", "SearchKeywords", "Home")</li>
                </ul>
            </div>
        </div>
        <div style="background-color: #fff">
            <div id="navWrap">
                <div id="panelbar-admin">
                </div>
                @RenderSection("Navbar", false)</div>
            <div class='centerWrap floatWrap widgetOverview' id="mainWrap">
                <div id="exampleTitleHeader">
                    <p id="exampleTitle">
                        @RenderSection("Title", false)<div style="margin-right: 15px; margin-top: -40px; text-align: right">@RenderSection("Links", false)</div>
                    </p>
                </div>
                <div id="mainWrapInner">
                    <div id="main">
                        <div id="example">@RenderBody()
                        </div>
                    </div>
                </div>
            </div>
            <div id="footer">
                <div class="centerWrap">
                    <p id="copy">
                        Copyright &copy; 2012 SEMPLEST</p>
                </div>
            </div>
        </div>
    </body>
</html>