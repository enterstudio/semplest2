@model IEnumerable<Semplest.Admin.Models.sp_GetRigtsRolesInteraction_Result>
@{
    ViewBag.Title = "Edit";
}

<h2>Edit Roles for @ViewBag.RoleName</h2>
@section Navbar{
    <script src="@Url.Content("~/Scripts/PanelBar.js")" type="text/javascript"></script>
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>
        <legend></legend>
        <div class= "rights">
             @Html.Partial("_RolesRightsAssociation", Model)
        </div>
        <p>
            <input type="submit" value="Save" />
        </p>
           
    </fieldset>
}

<div>
    @Html.ActionLink("Back to Roles List", "Index")
</div>
