<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - SEMPLEST ADMIN</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/kendo.common.min.css")" rel="Stylesheet" />
    <link href="@Url.Content("~/Content/kendo.default.min.css")"  rel="Stylesheet" />
    <link href="@Url.Content("~/Content/kendo.dataviz.min.css")"  rel="Stylesheet" />
    <link href="@Url.Content("~/Content/kendo.metro.min.css")"  rel="Stylesheet" />
    @*
    <link href="@Url.Content("~/Content/example.css")"  rel="Stylesheet" />
*@
    <script src="@Url.Content("~/Scripts/kendo.all.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/example.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/console.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/prettify.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                <h1>Semplest Admin Application</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <nav>
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                </ul>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
